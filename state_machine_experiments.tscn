[sub_resource type="GDScript" id="GDScript_os458"]
script/source = "@tool
extends U
@warning_ignore(\"unused_parameter\")

var patrol_point_0 = Vector2(700, 20)
var patrol_point_1 = Vector2(700, 300)

var direction = 1
var look_at_node: Node2D = null
var player = U.group(\"player\")[0]

func run():
	var enemy = U.group(\"enemy\")[0]
	if look_at_node == null:
		look_at_node = enemy.find_child(\"Look\")
	if enemy.position.y > patrol_point_1.y:
		direction = -1
	if enemy.position.y < patrol_point_0.y:
		direction = 1
	enemy.position.y += direction
	var area = look_at_node.get_child(0)

	for body in area.get_overlapping_bodies():
		
		if body == player:
			return true
	return false
	
"

