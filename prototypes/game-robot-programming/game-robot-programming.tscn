[sub_resource type="GDScript" id="GDScript_trcyf"]
script/source = "@tool
extends U
@warning_ignore(\"unused_parameter\")
func run(from: Button):
	var areas = from.get_node(\"../Area2D2\").get_overlapping_areas()
	
	areas.sort_custom(func (a: Area2D, b: Area2D): return a.position.y < b.position.y)
	
	for area in areas:
		var stmt = area.get_node(\"Code\").get_code()
		if stmt == \"forward\":
			print(\"forward\")
		elif stmt == \"while\":
			print(\"loop\")
			print(area.get_node(\"Code\").get_stmts())
"

