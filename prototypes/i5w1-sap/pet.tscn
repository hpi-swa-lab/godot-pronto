[sub_resource type="GDScript" id="GDScript_wy3cy"]
script/source = "@tool
extends U
@warning_ignore(\"unused_parameter\")
func run(other, direction, from, to):
	if to.to_string().contains(\"Health\"): # ???
		to = to.get_parent()
	if not (to.is_in_group(\"our_pets\") and other.is_in_group(\"their_pets\") \\
		or to.is_in_group(\"their_pets\") and other.is_in_group(\"our_pets\")):
			return false
	if to.is_in_group(\"dead\") or other.is_in_group(\"dead\"):
		return false
	return true
"

